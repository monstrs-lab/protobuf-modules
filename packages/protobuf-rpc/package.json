{
  "name": "@monstrs/protobuf-rpc",
  "version": "0.0.3",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "proto",
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist",
    "proto:generate": "buf generate proto",
    "proto:lint": "buf lint proto"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.15.0-1",
    "@bufbuild/protobuf": "^1.2.0",
    "@bufbuild/protoc-gen-connect-es": "^0.8.6",
    "@bufbuild/protoc-gen-es": "^1.2.0"
  },
  "peerDependencies": {
    "@bufbuild/protobuf": "*"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  }
}
